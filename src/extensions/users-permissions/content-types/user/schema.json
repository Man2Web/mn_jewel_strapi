{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "userCart": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "favourites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "digital_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::digital-order.digital-order",
      "mappedBy": "user"
    },
    "tOtp": {
      "type": "string",
      "maxLength": 6,
      "minLength": 6,
      "unique": true
    },
    "scheme_transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::scheme-transaction.scheme-transaction",
      "mappedBy": "user"
    },
    "user_schemes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-scheme.user-scheme",
      "mappedBy": "user"
    }
  }
}
