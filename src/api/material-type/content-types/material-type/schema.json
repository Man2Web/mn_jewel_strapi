{
  "kind": "collectionType",
  "collectionName": "material_types",
  "info": {
    "singularName": "material-type",
    "pluralName": "material-types",
    "displayName": "Material Type",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "material_type": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "material_type"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "homepageBanner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "isHomePageBanner": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isMobileNavbar": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "navabarImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "digitalOrder": {
      "type": "boolean",
      "required": true
    },
    "digitalIcon": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
